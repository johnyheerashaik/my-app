# Copy to .env and fill in your secret key (do NOT commit .env)
OPENAI_API_KEY=your_openai_api_key_here

# Optional
OPENAI_MODEL=gpt-4o-mini
PORT=8787

# Maximum number of tool-calling steps the agent can take (default: 200)
# Essentially unlimited - agent can handle very complex multi-step tasks
AGENT_MAX_STEPS=200
